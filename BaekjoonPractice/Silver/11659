N, M = map(int, input().split(" "))
num = list(map(int, input(). split(" ")))

for x in range(M):
    start, end = map(int, input().split(" "))
    total = 0
    #print(str(start) + " " + str(end))
    for i in range(start-1, end):
        #print("Num: " + str(i))
        total += num[i]
    print(total)
